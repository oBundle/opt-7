{"components/ob-custom/ob-footer":"{{#if banners.bottom}}\r\n    {{#if settings.data_tag_enabled}}\r\n        <div class=\"banners\" data-banner-location=\"bottom\">\r\n            {{#each (limit banners.bottom_metadata 1)}}\r\n                <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\" data-position=\"{{this.location}}\" data-name=\"{{this.banner-name}}\">\r\n                    <div data-event-type=\"promotion-click\">\r\n                        {{{this.content}}}\r\n                    </div>\r\n                </div>\r\n            {{/each}}\r\n        </div>\r\n    {{else}}\r\n        <div class=\"banners\" data-banner-location=\"bottom\">\r\n            <div class=\"banner\">\r\n                {{{limit banners.bottom 1}}}\r\n            </div>\r\n        </div>\r\n    {{/if}}\r\n{{/if}}\r\n<footer class=\"footer\" role=\"contentinfo\">\r\n    <div class=\"small-container\">\r\n        <section class=\"footer-info\">\r\n            <article class=\"footer-info-col footer-info-col--small\">\r\n                <h5 class=\"footer-info-heading\">{{lang 'footer.categories'}} <svg class=\"footer-arrow-down\"><use xlink:href=\"#icon-arrow-down-mobile\"></use></svg></h5>\r\n                <ul class=\"footer-info-list\">\r\n                  {{#each (limit categories 7)}}\r\n                      <li>\r\n                          <a href=\"{{url}}\">{{name}}</a>\r\n                      </li>\r\n                  {{/each}}\r\n                  <li><a href=\"/categories\">{{lang 'common.view_all'}}</a></li>\r\n                </ul>\r\n            </article>\r\n\r\n            <article class=\"footer-info-col footer-info-col--small\">\r\n                <h5 class=\"footer-info-heading\">{{lang 'footer.navigate'}} <svg class=\"footer-arrow-down\"><use xlink:href=\"#icon-arrow-down-mobile\"></use></svg></h5>\r\n                <ul class=\"footer-info-list\">\r\n                  {{#each (limit pages 8)}}\r\n                      <li>\r\n                          <a href=\"{{url}}\">{{name}}</a>\r\n                      </li>\r\n                  {{/each}}\r\n                  <li><a href=\"/categories\">{{lang 'common.view_all'}}</a></li>\r\n                </ul>\r\n            </article>\r\n\r\n\r\n            <article class=\"footer-info-col newsletter\" data-section-type=\"newsletterSubscription\">\r\n                <div class=\"FooterNewsletter\">\r\n                  {{#if settings.show_newsletter_box}}\r\n                      {{> components/common/subscription-form footer}}\r\n                  {{/if}}\r\n                </div>\r\n                <div class=\"footer-info-col footer-info-col--social\">\r\n                    <h5 class=\"footer-info-heading\">{{lang 'social.connect'}}</h5>\r\n                    {{> components/common/social-links}}\r\n                </div>\r\n\r\n            </article>\r\n        </section>\r\n        </div>\r\n        <div class=\"HomeBottomSection\">\r\n          <div class=\"container\">\r\n            <div class=\"footer-copyright\">\r\n                {{#if theme_settings.geotrust_ssl_common_name}}\r\n                    <div class=\"footer-geotrust-ssl-seal\">\r\n                        {{> components/common/geotrust-ssl-seal}}\r\n                    </div>\r\n                {{/if}}\r\n                <p class=\"powered-by\">&copy; {{moment format=\"YYYY\"}} {{settings.store_name}}. All Rights Reserved.</p>\r\n            </div>\r\n\r\n            <div class=\"footer-copyright\">\r\n                <div class=\"payment-icons\">\r\n                  {{> components/ob-custom/ob-payment-icons}}\r\n                </div>\r\n                <p class=\"powered-by\">Exclusive Development Partner <a href=\"https://obundle.com/\" target=\"_blank\"><img src=\"{{cdn 'img/footer-logo-oBundle.png'}}\" alt=\"\"></a></p>\r\n            </div>\r\n\r\n        </div>\r\n    </div>\r\n</footer>\r\n","components/common/subscription-form":"<h5 class=\"footer-info-heading\">{{lang 'newsletter.subscribe'}}</h5>\n<p><span>Get 10% off your first order </span>and find out about our latest deals & sales</p>\n\n<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\n    <fieldset class=\"form-fieldset\">\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n        <input type=\"hidden\" name=\"check\" value=\"1\">\n        <div class=\"form-field\">\n            <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\n            <div class=\"form-prefixPostfix wrap\">\n                <input class=\"form-input\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"{{customer.email}}\" placeholder=\"{{lang 'newsletter.email_placeholder'}}\">\n                <input class=\"button button--primary form-prefixPostfix-button--postfix\" type=\"submit\" value=\"{{lang 'newsletter.subscribe_submit'}}\">\n            </div>\n            {{#if settings.show_newsletter_summary }}\n                <div class=\"footer-newsletter-summary\">{{settings.newsletter_summary}}</div>\n            {{/if}}\n        </div>\n    </fieldset>\n</form>\n","components/common/social-links":"{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            <li class=\"socialLinks-item\">\n                {{#if display_name '===' 'google+'}}\n                    <a class=\"icon icon--google\" href=\"{{url}}\" target=\"_blank\">\n                        <svg><use xlink:href=\"#icon-google\" /></svg>\n                    </a>\n                {{else}}\n                    <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\">\n                        <!-- <svg><use xlink:href=\"#icon-{{display_name}}\" /></svg> -->\n                        <i class=\"fa fa-{{display_name}}\"></i>\n                    </a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n{{/if}}\n","components/common/geotrust-ssl-seal":"<table width=\"135\" border=\"0\" cellpadding=\"2\" cellspacing=\"0\" title=\"Click to Verify - This site chose GeoTrust SSL for secure e-commerce and confidential communications.\">\n    <tr>\n        <td width=\"135\" align=\"center\" valign=\"top\">\n            <script type=\"text/javascript\" src=\"https://seal.geotrust.com/getgeotrustsslseal?host_name={{theme_settings.geotrust_ssl_common_name}}&amp;size={{theme_settings.geotrust_ssl_seal_size}}&amp;lang={{locale_name}}\"></script><br />\n            <a href=\"http://www.geotrust.com/ssl/\" target=\"_blank\"  style=\"color:#000000; text-decoration:none; font:bold 7px verdana,sans-serif; letter-spacing:.5px; text-align:center; margin:0px; padding:0px;\"></a>\n        </td>\n    </tr>\n</table>\n","components/ob-custom/ob-payment-icons":"<svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-footer-icon-payment\"></use></svg>\r\n"}